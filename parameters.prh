	end
end

subsection Discretization
	set refinement = global
	set XY = 2
	set Z = 3
end

subsection Assembly
	set Threads = 4
end

subsection Solver
	set Solver = UMFPACK
	set GMRESSteps = 500
	set Preconditioner = Jacobi
	set PreconditionerBlockCount = 20
	set Steps = 90000
	set Precision = 1e-6
end

subsection Constants
	set AllOne = true
	set EpsilonZero = 8.854e-18
