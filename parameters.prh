subsection Output
		set Output Grid = false
		set Output Dofs = true
		set Output Active Cells = true
		set Verbose Output = true
end

subsection Measures
	subsection Connectors
		set Type in = Circle
		set Type out = Circle
		set Radius in = 1.5
		set Radius out = 1.5
	end
	subsection Region
		set XLength = 10
		set YLength = 10
		set ZLength = 10
	end
	subsection Waveguide
		set Delta = 0
		set epsilon in = 2.2
		set epsilon out = 1.0
		set Lambda = 5.6328
	end
	subsection Boundary Conditions
		set Type = PML
		set XY in = 20
		set XY out = 20
		set Mantle = 20
		set KappaXMax = 10.0
		set KappaYMax = 10.0
		set KappaZMax = 10.0
		set SigmaXMax = 10.0
		set SigmaYMax = 10.0
		set SigmaZMax = 10.0
